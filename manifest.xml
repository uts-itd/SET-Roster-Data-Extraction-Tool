<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<OfficeApp xmlns="http://schemas.microsoft.com/office/appforoffice/1.1" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:bt="http://schemas.microsoft.com/office/officeappbasictypes/1.0" xmlns:ov="http://schemas.microsoft.com/office/taskpaneappversionoverrides" xsi:type="TaskPaneApp">
  <Id>b395362a-f435-436f-b9b9-3e9552ff0967</Id>
  <Version>1.0.0.0</Version>
  <ProviderName>Lifetime Learner Experience Unit</ProviderName>
  <DefaultLocale>en-US</DefaultLocale>
  <DisplayName DefaultValue="SET Roster Data Extraction Tool"/>
  <Description DefaultValue="A tool to extract roster data into a table."/>
  <IconUrl DefaultValue="https://uts-itd.github.io/SET-Roster-Data-Extraction-Tool/assets/UTS_logo_web-32.png"/>
  <HighResolutionIconUrl DefaultValue="https://uts-itd.github.io/SET-Roster-Data-Extraction-Tool/assets/UTS_logo_web-64.png"/>
  <SupportUrl DefaultValue="https://studentutsedu.sharepoint.com/sites/SET-roster-data-extraction-tool"/>
  <AppDomains>
    <AppDomain>https://uts-itd.github.io</AppDomain>
  </AppDomains>
  <Hosts>
    <Host Name="Workbook"/>
  </Hosts>
  <DefaultSettings>
    <SourceLocation DefaultValue="https://uts-itd.github.io/SET-Roster-Data-Extraction-Tool/taskpane.html"/>
  </DefaultSettings>
  <Permissions>ReadWriteDocument</Permissions>
  <VersionOverrides xmlns="http://schemas.microsoft.com/office/taskpaneappversionoverrides" xsi:type="VersionOverridesV1_0">
    <Hosts>
      <Host xsi:type="Workbook">
        <DesktopFormFactor>
          <FunctionFile resid="SRDET.Url"/>
          <ExtensionPoint xsi:type="PrimaryCommandSurface">
            <OfficeTab id="TabHome">
              <Group id="SRDET">
                <Label resid="SRDET.Label"/>
                <Icon>
                  <bt:Image size="16" resid="Icon.16x16"/>
                  <bt:Image size="32" resid="Icon.32x32"/>
                  <bt:Image size="80" resid="Icon.80x80"/>
                </Icon>

				<Control xsi:type="Button" id="ExtractDataButton">
					<Label resid="ExtractButton.Label"/>
					<Supertip>
						<Title resid="ExtractButton.Label"/>
						<Description resid="ExtractButton.Tooltip"/>
					</Supertip>
					<Icon>
						<bt:Image size="16" resid="Icon.16x16"/>
						<bt:Image size="32" resid="Icon.32x32"/>
						<bt:Image size="80" resid="Icon.80x80"/>
                  	</Icon>
					<Action xsi:type="ExecuteFunction">
						<FunctionName>extractData</FunctionName>
					</Action>
				</Control>

                <Control xsi:type="Button" id="AboutButton">
                  <Label resid="AboutButton.Label"/>
                  <Supertip>
                    <Title resid="AboutButton.Label"/>
                    <Description resid="AboutButton.Tooltip"/>
                  </Supertip>
                  <Icon>
                    <bt:Image size="16" resid="Icon.16x16"/>
                    <bt:Image size="32" resid="Icon.32x32"/>
                    <bt:Image size="80" resid="Icon.80x80"/>
                  </Icon>
                  <Action xsi:type="ShowTaskpane">
                    <TaskpaneId>ButtonId1</TaskpaneId>
                    <SourceLocation resid="About.Url"/>
                  </Action>
                </Control>
              </Group>
            </OfficeTab>
          </ExtensionPoint>
        </DesktopFormFactor>
      </Host>
    </Hosts>
    <Resources>
      <bt:Images>
        <bt:Image id="Icon.16x16" DefaultValue="https://uts-itd.github.io/SET-Roster-Data-Extraction-Tool/assets/UTS_logo_web-16.png"/>
        <bt:Image id="Icon.32x32" DefaultValue="https://uts-itd.github.io/SET-Roster-Data-Extraction-Tool/assets/UTS_logo_web-32.png"/>
        <bt:Image id="Icon.80x80" DefaultValue="https://uts-itd.github.io/SET-Roster-Data-Extraction-Tool/assets/UTS_logo_web-80.png"/>
      </bt:Images>
      <bt:Urls>
        <bt:Url id="SRDET.Url" DefaultValue="https://uts-itd.github.io/SET-Roster-Data-Extraction-Tool/commands.html"/>
        <bt:Url id="About.Url" DefaultValue="https://uts-itd.github.io/SET-Roster-Data-Extraction-Tool/taskpane.html"/>
      </bt:Urls>
      <bt:ShortStrings>
        <bt:String id="SRDET.Label" DefaultValue="SET Roster Data Extraction Tool"/>
        <bt:String id="AboutButton.Label" DefaultValue="About"/>
		<bt:String id="ExtractButton.Label" DefaultValue="Extract Data"/>
      </bt:ShortStrings>
      <bt:LongStrings>
        <bt:String id="AboutButton.Tooltip" DefaultValue="About the SET Roster Data Extraction Tool"/>
		<bt:String id="ExtractButton.Tooltip" DefaultValue="Extract Data from a SET Roster"/>
      </bt:LongStrings>
    </Resources>
  </VersionOverrides>
</OfficeApp>
